  "description": "Backend do aplicativo!",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register ./src/tests/*$NAME*.{test,spec}.ts -t 10000 --exit",
    "recreateDB": "npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "prestart": "tsc && npm run recreateDB",
    "start": "node ./dist/src/server.js",
    "dev": "ts-node-dev --respawn --ignore-watch node_modules src/server.ts"
  },
